{
  "expo": {
    "name": "Gym Planner",
    "slug": "gym-planner",
    "version": "0.1.0",
    "orientation": "portrait",
    "scheme": "gym-planner",
    "newArchEnabled": true,
    "platforms": ["ios", "android"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.gymplanner.app"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "package": "com.gymplanner.app"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    }
  }
}
