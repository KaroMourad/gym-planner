generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Workout {
  id        String   @id @default(uuid()) @db.Uuid
  name      String   @db.VarChar(120)
  createdAt DateTime @default(now()) @map("created_at")

  @@map("workouts")
}
